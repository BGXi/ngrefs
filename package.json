{
  "name": "ng-cli-ref",
  "version": "0.0.0",
  "scripts": {
    "clean": "rimraf ./dist",
    "main:serve": "ng run main:serve",
    "main:build": "ng run main:build:production",
    "main:sitemap": "node sitemap-generator.js",
    "main:server:build": "ng run main:server",
    "main:server:bundle": "webpack --config ./projects/main/webpack.server.config.js --progress --colors",
    "premain:server:serve": "npm run clean && npm run main:build && npm run main:server:build && npm run main:server:bundle",
    "main:server:serve": "node ./dist/server",
    "main:docker:build": "docker build -t registry.gitlab.com/navix/dhub/ngrefs/main .",
    "main:docker:push": "docker push registry.gitlab.com/navix/dhub/ngrefs/main",
    "main:deploy": "npm run clean && npm run main:sitemap && npm run main:build && npm run main:server:build && npm run main:server:bundle && npm run main:docker:build && npm run main:docker:push",
    "editor:serve": "ng run editor:serve",
    "editor:docgen:angular": "docgen -p ./sources/angular/packages/ -o ./projects/editor/src/app/apis/angular.json -c ./docgen.json",
    "editor:docgen:angular-cli": "cpx \"./sources/angular-cli/packages/angular/cli/lib/config/schema.json\" \"./projects/editor/src/app/apis\"",
    "posteditor:docgen:angular": "node ./projects/editor/angular-api-post-gen.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.5",
    "@angular/common": "^8.2.5",
    "@angular/compiler": "^8.2.5",
    "@angular/core": "^8.2.5",
    "@angular/forms": "^8.2.5",
    "@angular/http": "^7.2.15",
    "@angular/platform-browser": "^8.2.5",
    "@angular/platform-browser-dynamic": "^8.2.5",
    "@angular/platform-server": "^8.2.5",
    "@angular/router": "^8.2.5",
    "@ngx-kit/core": "^7.3.2",
    "@nvxme/monit-client": "^2.1.0",
    "@nvxme/monit-ng-client": "^1.0.1",
    "@nvxme/ngx-md-render": "^1.0.0-alpha.5",
    "core-js": "^3.2.1",
    "file-saver": "^2.0.2",
    "hammerjs": "^2.0.8",
    "rxjs": "^6.5.3",
    "sass-mq": "^5.0.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.3",
    "@angular-devkit/build-ng-packagr": "^0.803.3",
    "@angular/cli": "^8.3.3",
